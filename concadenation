#include<stdio.h>
#include<stdlib.h>
#include<string.h>
int main()

{
    
    char s1[20];
    char s2[20];
    printf("Enter String 1");
    scanf("%s",s1);
    
    printf("Enter String 2");
    scanf("%s",s2);
    int c;
    char ch;
    for(int i=0;s1[i]!='\0';i++)
    {
       c++;
       ch=s1[i];
       
    }
    if(ch==s2[0])
    
    {
        for(int i=1;s2[i]!='\0';i++)
        {
            
            s1[c]=s2[i];
            c++;
        }
        
    }
    else
    {
     for(int i=0;s2[i]!='\0';i++)
        {
            
            s1[c]=s2[i];
            c++;
        }
    }
    printf("\n%s",s1);
    
    return 0;
    
}

